    ShipMonk\PHPStan\Rule\ForbidNullInBinaryOperationsRule:
        phpstan.rules.rule: %shipmonkRules.forbidNullInBinaryOperations.enabled%
    ShipMonk\PHPStan\Rule\ForbidNullInInterpolatedStringRule:
