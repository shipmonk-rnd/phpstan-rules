    ShipMonk\PHPStan\Rule\ForbidIncrementDecrementOnNonIntegerRule:
        phpstan.rules.rule: %shipmonkRules.forbidIncrementDecrementOnNonInteger.enabled%
    ShipMonk\PHPStan\Rule\ForbidMatchDefaultArmForEnumsRule:
